<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
    <suppress files=".*" checks="JavadocPackage"/>
    <suppress files=".*" checks="JavadocStyle"/>
    <suppress files=".*" checks="JavadocVariable"/>
    <suppress files=".*" checks="JavadocMethod"/>
    <suppress files=".*" checks="JavadocType"/>

    <suppress checks="[a-zA-Z0-9]*" files=".*\.png"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.jpg"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.jpeg"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.gif"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.svg"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.ico"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.robot"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.generated-sources.*"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.package-info.java"/>
    <suppress checks="[a-zA-Z0-9]*" files=".*\.properties"/>


</suppressions>